{
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "amd": true,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "comma-dangle": 1,
      "quotes": [ 1, "single" ],
      "no-undef": 0,
      "global-strict": 0,
      "semi": [ "error", "never"],
      "no-extra-semi": 1,
      "no-underscore-dangle": 0,
      "no-console": 0,
      "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
      "no-trailing-spaces": [0, { "skipBlankLines": true }],
      "no-unreachable": 1,
      "no-alert": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1
    }
  }
  